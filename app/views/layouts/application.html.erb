<!DOCTYPE html>
<html>
<head>
  <title>Aguilas Guadalupanas</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="main" class="website">
    <table id="header" class="website">
      <tr><td align=center>
      <%= render 'general/header' %>
      </td></tr>
    </table>
    <div id="menu" class="website">
      <%= render 'general/menu' %>
    </div>
    <div id="content" class="website">
      <%= yield %>
    </div>
    <div id="footer">
      <%= render 'general/footer' %>
    </div>
  </div>
</body>
</html>
