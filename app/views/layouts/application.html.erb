<!DOCTYPE html>
<html>
<head>
  <title>Website</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="main" class="website">
    <div id="header" class="website">
      <%= render 'general/header' %>
    </div>
    <div id="menu" class="website">
      <%= render 'general/menu' %>
    </div>
    <div id="content" class="website">
      <%= yield %>
    </div>
    <div id="extras" class="website">
      <%= render 'general/extras' %>
    </div>
    <div id="footer">
      <%= render 'general/footer' %>
    </div>
  </div>
</body>
</html>
